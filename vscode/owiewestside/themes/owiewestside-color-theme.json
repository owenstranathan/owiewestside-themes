	"tokenColors": [
		{
			"name": "No HighLight",
			"scope":[
				"entity",
				"keyword.operator",
				"storage.modifier.pointer",
				"storage.modifier.reference",
				"punctuation.accessor",
				// "storage.type.cs",
			],
			"settings": {
				"foreground": "#cccccc",
			}
		},
		{
			"name": "Statement",
			"scope": [
				"keyword.control",
				"keyword.other.typeof.cs",
				"storage.modifier.cs",
			],
			"settings": {
				"foreground": "#f9f1a5",
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#61d6d6"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
			],
			"settings": {
				"foreground": "#cccccc"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier",
			],
			"settings": {
				"foreground": "#15c60d"
			}
		},
		{
			"name": "include, preprocessor",
			"scope": [
				"meta.preprocessor",
				"meta.preprocessor.pragma",
				"keyword.control.directive",
				"entity.other.attribute-name.pragma",
				"entity.name.other.preprocessor",
				"keyword.other.using.cs",
				"keyword.control.import.python"
			],
			"settings": {
				"foreground": "#3b78ff"
			}
		},
		{
			"name": "Literal",
			"scope": [
				"string",
				"constant",
			],
			"settings": {
				"foreground": "#b4009e"
			}
		},
		{
			"name": "Escape and Format characters",
			"scope": [
				"constant.character.escape",
				"constant.character.format",
				"punctuation.definition.interpolation"
			],
			"settings": {
				"foreground": "#ffa336",
			}
		}
	]
}
	